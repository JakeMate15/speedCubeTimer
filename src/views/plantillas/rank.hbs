<div class="container-fluid">
    <h1 class="h3 mb-2 text-gray-800">Ranking</h1>

    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr>
            <th>Puesto</th>
            <th>Nombre</th>
            <th>Tiempo</th>
            <th>Mezcla</th>
            </tr>
        </thead>
        <tbody>
            {{#each info}}
            <tr>
            <td>{{@index}}</td>
            <td>{{Nombre}}</td>
            <td>{{tiempo}}</td>
            <td>{{mezcla}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>